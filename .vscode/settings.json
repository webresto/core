{
  "files.exclude": {
    // Hide all files that end in .d.ts
    "**/*.d.ts": true,
    // Hide all files that end with .js if they have a .ts file
    "**/*.1js": {
      "when": "$(basename).ts"
    },
  },
  "prettier.quoteProps": "consistent",
  "prettier.printWidth": 180,
  "json.schemas": [
    {
        "fileMatch": [
            "settings/*.json"
        ],
        "url": "./libs/schemas/settingsFile.json"
    }
  ]
}